{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/altsoph/EENLP/main/docs/data/models/_model.schema.json",
  "title": "Model",
  "description": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the model."
    },
    "description": {
      "type": "string"
    },
    "languages": {
      "type": "array",
      "description": "Languages.",
      "items": {
        "enum": [
          "Albanian",
          "Armenian",
          "Belarusian",
          "Bosnian",
          "Bulgarian",
          "Croatian",
          "Czech",
          "Estonian",
          "Georgian",
          "Hungarian",
          "Kazakh",
          "Latvian",
          "Lithuanian",
          "Macedonian",
          "Moldovan",
          "Moldovian",
          "Montenegrin",
          "Polish",
          "Romanian",
          "Russian",
          "Serbian",
          "Slovak",
          "Slovakian",
          "Slovenian",
          "Ukrainian"
        ]
      }
    },
    "type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "enum": [
            "BERT",
            "RoBERTa",
            "ELMo",
            "GPT2",
            "DistilBERT",
            "Electra",
            "static word embeddings"
          ]
        }
      ]
    },
    "huggingface": {
      "type": [
        "string",
        "null"
      ]
    },
    "paper": {
      "type": [
        "string",
        "null"
      ]
    },
    "citation": {
      "type": [
        "string",
        "null"
      ]
    },
    "cased": {
      "anyOf": [
        {
          "type": [
            "string",
            "null"
          ]
        },
        {
          "enum": [
            "uncased",
            "cased",
            "both"
          ]
        }
      ]
    },
    "pre-trained on": {
      "description": "",
      "oneOf": [
        {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "enum": [
                  "wikipedia",
                  "common crawl"
                ]
              }
            ]
          }
        },
        {
          "type": "null"
        }
      ]
    },
    "comments": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "name",
    "type",
    "languages"
  ],
  "additionalProperties": false
}
